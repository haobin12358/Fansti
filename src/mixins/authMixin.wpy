<script>
  import wepy from 'wepy';

  //  拿到windowHeight后设置合适的cardHeight
  export default class CardMixin extends wepy.mixin{
    props = {

    };

    data = {
      authUserType: '',
      authIsPd: false,
      authTodaySearch: false,
      authShowOrigin: false,
    };

    computed = {};

    methods = {};

    events = {};

    onLoad() {
      let userType = wx.getStorageSync('user_type');

      userType = userType.toString()
      this.authUserType = userType
      this.authIsPd = userType == 8
      this.authTodaySearch = ['0','3','8'].includes(userType)
      this.authShowOrigin = userType == 0 || userType == 2
    }
  }
</script>

